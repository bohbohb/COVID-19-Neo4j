import { OnInit } from '@angular/core';
import { FormBuilder, FormGroup } from '@angular/forms';
import { AngularNeo4jService } from './angular-neo4j.service';
export declare class AngularNeo4jComponent implements OnInit {
    private fb;
    private neo4j;
    loginForm: FormGroup;
    queryForm: FormGroup;
    driver: any;
    results: any;
    constructor(fb: FormBuilder, neo4j: AngularNeo4jService);
    ngOnInit(): void;
    createForms(): void;
    connect(): void;
    disconnect(): void;
    run(): void;
}
