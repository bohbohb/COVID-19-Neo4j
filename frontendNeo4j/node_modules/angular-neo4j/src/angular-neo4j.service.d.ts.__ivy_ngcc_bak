export declare class AngularNeo4jService {
    driver: any;
    constructor();
    /**
     * Create a new driver connection
     */
    connect(url: any, username: any, password: any, encrypted?: boolean): Promise<{}>;
    /**
     * Close a driver connection
     */
    disconnect(): void;
    /**
     * Get the last instantiated driver instance
     */
    getDriver(): any;
    /**
     * Create a new driver session
     */
    getSession(): any;
    /**
     * Run a query on the current driver
     */
    run(query: any, params?: any): any;
    private processInteger(integer);
    private processRecord(record);
}
